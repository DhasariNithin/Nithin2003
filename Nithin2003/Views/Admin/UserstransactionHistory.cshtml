@model IEnumerable<MyTransferMoney>
@{
    ViewData["Title"] = "UserstransactionHistory";
    Layout = "~/Views/Shared/_DashBoard.cshtml";
}

<!DOCTYPE html>
<html>

@*<head>
  *<meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Start your development with a Dashboard for Bootstrap 4.">
    <meta name="author" content="Creative Tim">
    <title>Argon Dashboard - Free Dashboard for Bootstrap 4</title>
    <!-- Favicon -->
    <link rel="icon" href="~/Argon-assets/img/brand/favicon.png" type="image/png">
    <!-- Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
    <!-- Icons -->
    <link rel="stylesheet" href="~/Argon-assets/vendor/nucleo/css/nucleo.css" type="text/css">
    <link rel="stylesheet" href="~/Argon-assets/vendor/fortawesome/fontawesome-free/css/all.min.css" type="text/css">
    <!-- Argon CSS -->
    <link rel="stylesheet" href="~/Argon-assets/css/argon.css?v=1.2.0" type="text/css">
    </head>*@
<body>
    <!-- Sidenav -->
    <nav class="sidenav navbar navbar-vertical  fixed-left  navbar-expand-xs navbar-light bg-white" id="sidenav-main">
        <div class="scrollbar-inner">
            <!-- Brand -->
            <div class="sidenav-header  align-items-center">
                <div class="sidenav-header  align-items-center">
                    <a class="navbar-brand" asp-controller="Home" asp-action="Index" type="button">
                        <img style="max-height: 60px;width:200px;" src="~/Argon-assets/img/Nithinnnnnn.png" class="navbar-brand-img" alt="...">
                    </a>
                </div>
            </div>
            <div class="navbar-inner">
                <!-- Collapse -->
                <div class="collapse navbar-collapse" id="sidenav-collapse-main">
                    <!-- Nav items -->
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link active" asp-controller="Admin" asp-action="Index">
                                <i class="ni ni-circle-08"></i>
                                <span class="nav-link-text">Admin Dashboard</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" asp-controller="Admin" asp-action="LoanAmountRequest">
                                <i class="ni ni-money-coins"></i>
                                <span class="nav-link-text">All Loan Requests</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" asp-controller="Admin" asp-action="UserstransactionHistory">
                                <i class="ni ni-money-coins"></i>
                                <span class="nav-link-text">All Transaction History</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" asp-controller="Admin" asp-action="ApproveAsAdmin">
                                <i class="ni ni-money-coins"></i>
                                <span class="nav-link-text">Add Admin/Editor</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" asp-controller="Dashboard" asp-action="UsersLoginDetails">
                                <i class="ni ni-circle-08"></i>
                                <span class="nav-link-text">All User Login History</span>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link active" asp-controller="Dashboard" asp-action="Index">
                                <i class="ni ni-tv-2 text-primary"></i>
                                <span class="nav-link-text">My Dashboard</span>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link active" asp-controller="Users" asp-action="EditDetails" asp-route-Username="@Context.Session.GetString("Username")">
                                <i class="ni ni-circle-08"></i>
                                <span class="nav-link-text">Edit My Profile</span>

                            </a>
                        </li>

                        <li class="nav-item">
                            <a style="background-color:#5e72e4;color:white" class="nav-link " asp-area="" asp-controller="Users" asp-action="SignOut">
                                <i class=""></i>
                                <span class="nav-link-text">Sign Out</span>
                            </a>
                        </li>
                    </ul>


                </div>
            </div>
        </div>
    </nav>
    <!-- Main content -->
    <div class="main-content" id="panel">

        <!-- Header -->
        <!-- Header -->
        <div class="header bg-primary pb-6">
            <div class="container-fluid">
                <div class="header-body">
                    <div class="row align-items-center py-4">
                        <div class="col-lg-6 col-7">
                            <h6 class="h2 text-white d-inline-block mb-0">Users Transaction</h6>
                            <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
                                <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
                                    <li class="breadcrumb-item"><a href="#"><i class="fas fa-home"></i></a></li>
                                    <li class="breadcrumb-item"><a href="#">Tables</a></li>
                                    <li class="breadcrumb-item active" aria-current="page">Tables</li>
                                </ol>
                            </nav>
                        </div>
                       @*  <div class="col-lg-6 col-5 text-right">
                            <a href="#" class="btn btn-sm btn-neutral">New</a>
                            <a href="#" class="btn btn-sm btn-neutral">Filters</a>
                        </div> *@
                    </div>
                </div>
            </div>
        </div>
        <!-- Page content -->
        <div class="container-fluid mt--6">
            <div class="row">
                <div class="col">
                    <div class="card">
                        <!-- Card header -->
                        <div class="card-header border-0">
                            <h3 class="mb-0">Users table</h3>
                        </div>
                        <!-- Light table -->
                        <div class="table-responsive">
                            <table class="table align-items-center table-flush">
                                <thead class="thead-light">
                                    <tr>
                                        <th>From User</th>
                                        <th>To User</th>
                                        <th>Amount Transfered</th>
                                        <th> Transaction Date</th>
                                            <th> IP Address</th>

                                    </tr>
                                </thead>
                                @foreach (var users in Model)
                                {
                                    <tbody class="list">
                                        <tr>
                                            <th scope="row">
                                                <div class="media align-items-center">

                                                    <div class="media-body">
                                                        <span class="name mb-0 text-sm">@users.FromUsername</span>
                                                    </div>
                                                </div>
                                            </th>

                                            <td>
                                                <span class="badge badge-dot mr-4">
                                                    <i class="bg-warning"></i>
                                                    <span class="status">@users.ToUsername</span>
                                                </span>
                                            </td>

                                            <td>
                                                <div class=" ni ni-money-coins">
                                                    <span class="">@users.Amount</span>

                                                </div>
                                            </td>
                                            <td>
                                                <span class="">@users.TransactionDate</span>
                                            </td>
                                            <td>
                                                <span class="">@users.IPAddress</span>
                                            </td>



                                    </tbody>
                                }
                            </table>
                        </div>

                    </div>
                </div>
            </div>

            <!-- Footer -->
            <footer class="footer pt-0">
                <div class="row align-items-center justify-content-lg-between">
                    <div class="col-lg-6">
                        @*<div class="copyright text-center  text-lg-left  text-muted">
                        &copy; 2020 <a href="https://www.creative-tim.com" class="font-weight-bold ml-1" target="_blank">Creative Tim</a>
                        </div>*@
                    </div>
                    <div class="col-lg-6">
                        <ul class="nav nav-footer justify-content-center justify-content-lg-end">

                            <li>
                                <a class="nav-link scrollto" asp-controller="Home" asp-action="ContactUs">Contact Us</a>
                            </li>

                        </ul>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    <!-- Argon Scripts -->
    <!-- Core -->
    <script src="~/Argon-assets/vendor/jquery/dist/jquery.min.js"></script>
    <script src="~/Argon-assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Argon-assets/vendor/js-cookie/js.cookie.js"></script>
    <script src="~/Argon-assets/vendor/jquery.scrollbar/jquery.scrollbar.min.js"></script>
    <script src="~/Argon-assets/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>
    <!-- Argon JS -->
    <script src="~/Argon-assets/js/argon.js?v=1.2.0"></script>
</body>

</html>

