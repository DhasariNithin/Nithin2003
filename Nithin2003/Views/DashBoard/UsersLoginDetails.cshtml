                    <div class="card">
                        <!-- Card header -->
                        <div class="card-header border-0">
                            <h3 class="mb-0">Users Ligin History</h3>
                        </div>
                        <!-- Light table -->
                        <div class="table-responsive">
                            <table class="table align-items-center table-flush">
                                <thead class="thead-light">
                                    <tr>

                                        <th>User name</th>
                                        <th> User Action</th>
                                        <th>Time</th>
                                        <th>User IP Address</th>

                                    </tr>
                                </thead>
                                @foreach (var users in Model)
                                {




                                    <tbody class="list">
                                        <tr>
                                            <td>
                                                <span class="name mb-0 text-sm">@users.UserName</span>
                                            </td>
                                            <td>
                                                <span>

                                                    <span class="status">@users.Action</span>
                                                </span>
                                            </td>
                                            <td>
                                                <div>

                                                    @users.Time
                                                </div>
                                            </td>
                                            <td>
                                                <span>@users.IPAddress</span>
                                            </td>

                                        </tr>

                                    </tbody>
                                }
                            </table>
                        </div>

                    </div>
                </div>
            </div>


